{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Sample CloudFormation Template for CSYE 6225 - Fall 2017",
    "Resources": {

      "HttpTargetGroup":{
        "Type":"AWS::ElasticLoadBalancingV2::TargetGroup",
        "Properties":{
          "Name":"httptest",
          "Port":"8080",
          "Protocol":"HTTP",
          "VpcId":{"Ref":"ParamVpcID"}
        }
      },
      "HttpsTargetGroup":{
        "Type":"AWS::ElasticLoadBalancingV2::TargetGroup",
        "Properties":{
          "Name":"httpstest",
          "Port":"8443",
          "Protocol":"HTTPS",
          "VpcId":{"Ref":"ParamVpcID"}
        }
      }
},
    "Parameters" : {
          "ParamVpcID" : {
            "Type" : "String"
       }
    }
}
